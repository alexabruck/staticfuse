{
  "name": "staticfuse-workspace",
  "private": true,
  "scripts": {
    "dev": "yarn workspace www develop",
    "start": "yarn workspace www start",
    "serve": "yarn workspace www serve",
    "build": "yarn workspace www build",
    "clean": "rm -rf www/.cache && rm -rf && find . -name \"node_modules\" -exec rm -rf '{}' +"
  },
  "workspaces": [
    "packages/*",
    "www"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0"
  }
}
